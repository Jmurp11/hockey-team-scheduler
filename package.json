  "scripts": {
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "serve": "nx serve",
    "start": "nx serve",
    "start:api": "nx serve api",
    "start:web": "nx serve web",
    "start:mobile": "nx serve mobile",
    "build:api": "nx build api",
    "build:web": "nx build web",
    "build:mobile": "nx build mobile",
    "build:tournament-etl": "nx build tournament-etl",
    "analyze:web": "nx build web --configuration=production --stats-json && npx webpack-bundle-analyzer dist/apps/web/stats.json",
    "analyze:mobile": "nx build mobile --configuration=production --stats-json && npx webpack-bundle-analyzer dist/apps/mobile/stats.json",
    "size:web": "nx build web --configuration=production && du -sh dist/apps/web && ls -lh dist/apps/web/browser/*.js | awk '{print $5, $9}'",
    "size:mobile": "nx build mobile --configuration=production && du -sh dist/apps/mobile && ls -lh dist/apps/mobile/browser/*.js | awk '{print $5, $9}'",
    "mobile:sync": "cd apps/mobile && npx cap sync",
    "mobile:android": "cd apps/mobile && npx cap open android",
    "mobile:ios": "cd apps/mobile && npx cap open ios",
    "graph": "nx graph",
    "affected": "nx affected",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "etl:start": "nx serve tournament-etl",
    "etl:leagues": "npx tsx tournament-etl/src/leagues/insertLeagues.ts",
    "etl:organizations": "npx tsx tournament-etl/src/organizations/insertOrgs.ts",
    "etl:rankings": "npx tsx tournament-etl/src/rankings/insertRankings.ts"
  },
